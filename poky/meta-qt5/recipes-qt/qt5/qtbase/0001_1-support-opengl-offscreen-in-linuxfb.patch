+    AndroidAssetDir(AAssetDir* ad)
+    {
+        if (ad) {
+            const char *fileName;
+            while ((fileName = AAssetDir_getNextFileName(ad)))
+                m_items.push_back(QString::fromUtf8(fileName));
+            AAssetDir_close(ad);
+        }
+    }
+    FilesList m_items;
+};
